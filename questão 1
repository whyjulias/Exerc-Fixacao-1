public class Paciente {

    int codigo; String nome, dataNascimento, sexo, planoSaude, alergia, tipoSanguineo;

    public Paciente(int cod, String nome, String data, String sexo, String planoSaude, String alergia, String tipo){

        this.codigo = cod;
        this.nome = nome;
        this.dataNascimento = data;
        this.sexo = sexo;
        this.planoSaude = planoSaude;
        this.alergia = alergia;
        this.tipoSanguineo = tipo;

    }
}
